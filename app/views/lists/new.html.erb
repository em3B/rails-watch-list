<%# navbar %>
<div class="navbar fixed-top">
  <h1 id="home"><%= link_to "🍿", lists_path %></h1>
  <h2 id="new-list"><%= link_to "New List", new_list_path %></h2>
</div>
<%# form %>
<h1>New List</h1>
<div id="form">
  <%= simple_form_for @list do |f| %>
    <div id="form-name">
      <%= f.label "Name" %>
      <%= f.text_field :name %>
    </div>
    <div id="form-btn">
      <%= f.submit %>
    </div>
  <% end %>
</div>
